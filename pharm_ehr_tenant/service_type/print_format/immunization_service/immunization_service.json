 "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"Custom HTML\", \"options\": \"<div class=\\\"print-heading\\\">\\t\\t\\t\\t<h2><div>Immunization Service</div><br><small class=\\\"sub-heading\\\">{{ doc.name }}</small>\\t\\t\\t\\t</h2></div>\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"_custom_html\", \"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldtype\": \"HTML\", \"options\": \"<!doctype html>\\n<html lang=\\\"en\\\">\\n  <head>\\n    <meta charset=\\\"utf-8\\\">\\n    <meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1\\\">\\n    <title>Bootstrap demo</title>\\n    <link href=\\\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css\\\" rel=\\\"stylesheet\\\" integrity=\\\"sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH\\\" crossorigin=\\\"anonymous\\\">\\n  </head>\\n  <body>\\n    <div class=\\\"main-div\\\" style=\\\"margin-top: 35px;\\\">\\n        <div class=\\\"row\\\">\\n            <div class=\\\"col-6\\\">\\n                {% set pat = frappe.get_doc(\\\"Patient\\\", doc.patient_name)%}\\n                <div style=\\\"font-size: 10px;\\\">Name(nombre): <b>{{pat.patient_name}}</b></div>\\n                <div style=\\\"font-size: 10px;\\\">Birth Date (fecha de nacimiento): <span class=\\\"p-1 text-center\\\"><b>{{pat.dob}}</b></span></div>\\n                <div style=\\\"font-size: 10px;\\\">Gender (genero): <span class=\\\" p-1 text-center\\\"><b>{{pat.sex}}</b></span></div>\\n                <div style=\\\"font-size: 10px;\\\">Vaccine Reactions (reacciones a la vacuna): </div>\\n            </div>\\n            <div class=\\\"col-6\\\" style=\\\"padding-left: 3%;\\\">\\n                <div style=\\\"font-size: 10px;\\\">Printed by (impress por): <span><b>{{doc.form_reviewed_by}}</b></span></div>\\n                <div style=\\\"font-size: 10px;\\\">Allergies (alergias): </div>\\n            </div>\\n            <div class=\\\"col-7 mt-3\\\">\\n                <div style=\\\"display: flex;\\\"><b style=\\\"font-weight: bold; font-size: 12px; \\\">IMMUNIZATION RECORD</b > <p style=\\\"font-size: 10px; margin-left: 2px;\\\">Comprobante de Inmunization</p></div>\\n            </div>\\n            <div class=\\\"col-4 mt-3\\\" >\\n                <div style=\\\"display: flex;\\\"><p style=\\\"font-size: 10px; \\\">Date Printed (fecha impresa): </p><b style=\\\"font-size: 10px; margin-top: 4px;\\\">{{frappe.utils.getdate()}}</b></div>\\n            </div>\\n        </div>\\n        <div class=\\\"table-div mt-4\\\">\\n            <table style=\\\"width: 100%;\\\">    \\n                <thead> \\n                    <tr>\\n                        <td scope=\\\"col\\\" class=\\\"head\\\" style=\\\"width: 10%; padding: 2px;\\\"><b style=\\\"font-size: 10PX; font-weight: bold;\\\">GROUP </b> <br/> <p style=\\\"font-size: 9px;\\\">groupo</p></td>\\n                        <td scope=\\\"col\\\" class=\\\"head\\\" style=\\\"width: 10%; padding: 2px;\\\"><b style=\\\"font-size: 10PX;font-weight: bold;\\\">SERIES </b> <br/><p style=\\\"font-size: 9px;\\\">serie</p></td>\\n                        <td scope=\\\"col\\\" class=\\\"head\\\" style=\\\"width: 10%; padding: 2px;\\\"><b style=\\\"font-size: 10PX;font-weight: bold;\\\">DATE GIVEN  </b><br/><p style=\\\"font-size: 9px;\\\">fecha de <br/> vacunacion</p></td>\\n                        <td scope=\\\"col\\\" class=\\\"head\\\" style=\\\"width: 20%; padding: 2px;\\\"><b style=\\\"font-size: 10PX;font-weight: bold;\\\">VACCINE </b><br/><p style=\\\"font-size: 9px;\\\">vacuna</p></td>\\n                        <td scope=\\\"col\\\" class=\\\"head\\\" style=\\\"width: 20%; padding: 2px;\\\"><b style=\\\"font-size: 10PX;font-weight: bold;\\\">#Lot </b><br/><p style=\\\"font-size: 9px;\\\"></p></td>\\n                        <td scope=\\\"col\\\" class=\\\"head\\\" style=\\\"width: 40%; padding: 2px;\\\"><b style=\\\"font-size: 10PX;font-weight: bold;\\\">CLINIC THAT ADMINISTERED OR TRANSCRIBED </b> <br/><p style=\\\"font-size: 9px;\\\">clinica que la administrio o transcribio</p></td>\\n                    </tr>\\n                </thead>\\n                <tbody>\\n                    {% if doc.vaccine_details %}\\n                        {% for row in doc.vaccine_details %}\\n                            {% if row.vaccine %}\\n                            <tr>\\n                                {% set med = frappe.get_doc(\\\"Medication\\\", row.vaccine) %}\\n                                <td style=\\\"font-size: 10px;\\\">{{med.medication_class}}</td>\\n                                <td style=\\\"font-size: 10px;\\\">{{row.site}}</td>\\n                                <td style=\\\"font-size: 10px;\\\">{{row.vaccine_given}}</td>\\n                                <td style=\\\"font-size: 10px;\\\">{{row.vaccine}}</td>\\n                                <td style=\\\"font-size: 10px;\\\">{{row.lot}}</td>\\n                                <td style=\\\"font-size: 10px;\\\">{{doc.form_reviewed_by}}</td>\\n                            </tr>\\n                            {% endif %}\\n                        {% endfor %}\\n                    {% else %}\\n                        <tr>\\n                            <td colspan=6 class= \\\"text-center\\\" style=\\\"font-size: 20px;\\\">\\n                                Record Not Found!\\n                            </td>\\n                        </tr>\\n                    {% endif %}\\n                </tbody>\\n            </table>\\n        </div>\\n    </div>\\n    <script src=\\\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js\\\" integrity=\\\"sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz\\\" crossorigin=\\\"anonymous\\\"></script>\\n  </body>\\n  <style>\\n    table, td{\\n        border: 1px solid black;\\n        border-collapse: collapse;\\n        text-align: c